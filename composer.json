{
    "name": "adamb/testimonial",
    "description": "Add a testimonial to the database and upload an image to the server",
    "type": "library",
    "require": {
        "adamb/database": "^1.0",
        "adamb/pagination": "^1.0",
        "adamb/image-upload": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggests": {
        "phpmailer/phpmailer": "^5.2; If you plan on sending an approval email on submission will need PHPMailer"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Binnersley",
            "email": "abinnersley@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Testimonials\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testimonials\\Tests\\": "tests/"
        }
    }
}
